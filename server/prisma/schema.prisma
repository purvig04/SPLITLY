generator client {
    provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
    id String @id @default(uuid())
    name String 
    email String @unique
    password String 
    contact String @map("phone_no")
    createdAt DateTime @default(now()) @map("created_at")
    updatedAt DateTime @default(now())

    @@map("users")
}